+MACHINE	"netbsd-*-80x86"
+AS		"/usr/bin/as"
+LD		"/usr/bin/ld -m elf_i386"
+CC		"/usr/bin/cc"
+CRT0		"--dynamic-linker /usr/libexec/ld.elf_so /usr/lib/crt0.o"
+CRT1		"/usr/lib/crti.o /usr/lib/crtbegin.o"
+CRTN		"/usr/lib/crtend.o /usr/lib/crtn.o"
+SYS_LIB        "-ltdf"
+SYS_LINK       "-L<PREFIX_LIB>/sys"
