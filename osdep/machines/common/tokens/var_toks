/* $Id$ */

/*
 * tcc environment implementing the keep list for var_toks.j
 */

/*
 * The -Yansi environment brings in c.tl, which is what this is used to build.
 * So, after loading -Yansi, +LIB is overridden here to remove -lc.
 */
+FLAG "-Yansi"
+LIB  "-lansi"

>FLAG_TLD	"-H token"
>FLAG_TLD	"-k token ~va_list"
>FLAG_TLD	"-k token ~__va_start"
>FLAG_TLD	"-k token ~va_arg"
>FLAG_TLD	"-k token ~va_end"
>FLAG_TLD	"-k token ~next_caller_offset"
>FLAG_TLD	"-k token ~next_callee_offset"

