/*
 * STARTUP FILE FOR COMPILATION MODE XC
 *
 * Xc represents strict ISO/IEC 9899:1999 compliance, no more, no less.
 *
 * $TenDRA$
 */

#pragma TenDRA declaration block Xc__startup begin

#pragma TenDRA begin

#pragma TenDRA longlong type allow

#include "../Literal/c99.pf"
#include "../Promotion/ansi.pf"

#define __ANDF__ 1
#define __TenDRA__ 1
#define __OPENLINT__ 1

#pragma TenDRA weak prototype analysis off
#pragma TenDRA incompatible interface declaration warning

#pragma TenDRA linkage resolution : (internal) warning

#pragma TenDRA unknown pragma allow
#pragma TenDRA no external declaration disallow

#ifndef __NO_PRINTF_CHECKS
#define __NO_PRINTF_CHECKS 1
#endif

/* Implicit function declarations are not allowed in C99 anymore. */
#pragma TenDRA implicit function declaration off
/* No implicit int for K&R-style function parameters. */
#pragma TenDRA implicit int type for function argument disallow
/* Implicit int type for functions lacking a return type was removed in C99. */
#pragma TenDRA implicit int type for function return disallow
/* C99 allows the same type qualifier to occur multiple times. */
#pragma TenDRA extra type qualifier allow
#pragma TenDRA extra type qualifier (typedef) allow
/* Declarations and statements can be mixed freely. */
#pragma TenDRA declaration after code allow
/* Initialisers for aggregate or union objects don't have to be constant
 * expressions anymore. */
#pragma TenDRA variable initialization of struct/union (auto) allow

/* 6.4.2.2  */
#pragma TenDRA identifier __func__ allow
/* 6.4.4.2 */
#pragma TenDRA hexadecimal float literal allow
/* 6.4.9#2 */
#pragma TenDRA line comment allow
/* 6.7.2 */
#pragma TenDRA keyword _Bool for keyword _Bool
/* 6.7.2#2 */
#pragma TenDRA implicit int type disallow
#pragma TenDRA implicit int type for const / volatile disallow
/* 6.7.2.1 */
#pragma TenDRA flexible array member allow
/* 6.7.2.2 */
#pragma TenDRA extra , allow
/* 6.7.3 */
#pragma TenDRA keyword restrict for keyword restrict
/* 6.7.4 */
#pragma TenDRA keyword inline for inline
#pragma TenDRA inline linkage external
/* 6.8.5.3 */
#pragma TenDRA for declaration allow
/* 6.8.6.4#1 */
#pragma TenDRA incompatible void return disallow
/* 6.10.3 */
#pragma TenDRA no space after macro disallow
#pragma TenDRA empty macro argument allow
#pragma TenDRA variable argument macro allow
/* 6.10.4 */
#pragma TenDRA option value "hash_line_number" 2147483647

#pragma TenDRA declaration block end
