/*
 *  STARTUP FILE FOR COMPILATION MODE Xc99
 *
 *  Xc99 represents strict ISO/IEC 9899:1999 compliance, no more, no less.
 *
 * $TenDRA$
 */
#pragma TenDRA declaration block Xc99__startup begin

#pragma TenDRA begin

#include "../Literal/ansi.pf"
#include "../Promotion/ansi.pf"

#define __ANDF__ 1
#define __TenDRA__ 1
#define __OPENLINT__ 1

#pragma TenDRA weak prototype analysis off
#pragma TenDRA incompatible interface declaration warning

#pragma TenDRA linkage resolution : (internal) warning

#pragma TenDRA unknown pragma allow
#pragma TenDRA no external declaration disallow

#ifndef __NO_PRINTF_CHECKS
#define __NO_PRINTF_CHECKS 1
#endif

/* Implicit function declarations are not allowed in C99 anymore. */ 
#pragma TenDRA implicit function declaration off
/* Implicit int type for functions lacking a return type was removed in C99. */
#pragma TenDRA implicit int type for function return disallow
/* C99 allows the same type qualifier to occur multiple times. */
#pragma TenDRA error "dcl_type_cv_dup" off
#pragma TenDRA error "dcl_type_type_cv" off
/* Declarations and statements can be mixed freely. */
#pragma TenDRA error "stmt_dcl_start" off
/* 6.4.9#2 */
#pragma TenDRA option "cpplus_comment" allow
/* 6.7.2#2 */
#pragma TenDRA implicit int type disallow
#pragma TenDRA implicit int type for const / volatile disallow
/* 6.7.2.2 */
#pragma TenDRA extra , allow
/* 6.8.6.4#1 */
#pragma TenDRA incompatible void return disallow

#pragma TenDRA declaration block end
