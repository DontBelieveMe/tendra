/* 
 * $TenDRA$
*/


/* Binaries */

+BIN_AS         "-BIN_AS-"
+BIN_AS1	"-BIN_AS1-"
+BIN_LD		"-BIN_LD-"


/* Libraries */

+LIB_CRTN	"-LIB_CRTN-"
+LIB_MCRT1	"-LIB_MCRT1-"
+LIB_CRT1	"-LIB_CRT1-"


+MACHINE		"irix-*-mips"
+FLAG			"--1MA"
+VERSION		"3.18"
+AS			"<BIN_AS> -O2"
+AS1			"<BIN_AS1> -O2 -pic2 -noglobal"
+LD			"<BIN_LD>"
+CRT0			""
+CRT1			"<LIB_CRT1> <TENDRA_LIBDIR>/sys/dyn_begin.o"
+CRTN			"<LIB_CRTN>"
