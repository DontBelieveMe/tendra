+MACHINE		"linux-*-80x86"
+AS			"/usr/bin/as -Qy"
+LD			"/usr/bin/ld -m elf_i386"
+CC			"/usr/bin/cc"
+CRT0			"-dynamic-linker /lib/ld-linux.so.1 /usr/lib/crt1.o"
+CRT1			"/usr/lib/crti.o /usr/lib/crtbegin.o"
+CRTN			"/usr/lib/crtend.o /usr/lib/crtn.o"
+SYS_LIB		"-ltdf"
+SYS_LINK		"-L<TENDRA_LIBDIR>/sys"
