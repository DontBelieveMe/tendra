# Standard directories and files
BASE=/u/g/release/Source
DEST=${BASE}/lib
UTIL=${BASE}/src/utilities
SRC=${UTIL}/make_mf
INIT=${SRC}/make_mf.ini
SRCX=${UTIL}/shared

# Standard variables

CC=tcc
TCCOPTS=-Yposix1 -Xs
CCOPTS=${TCCOPTS} -I${SRC} -I${SRCX} -DINIT_FILE=\"${INIT}\"
MAIN_CC=${CC} ${CCOPTS} -c
LINK_CC=${CC} ${TCCOPTS}
LIBS=

REMOVE=rm -f
MOVE=mv
COPY=cp
ECHO=echo
SHELL=/bin/sh

# Dependencies and construction rules

NAME=make_mf
EXTRA=${INIT}
EXTRA_CLEAN=
EXTRA_CLOBBER=

OFILES=depend.o error.o include.o main.o option.o output.o path.o\
 xalloc.o

${NAME} : ${OFILES}
	${LINK_CC} -o ${NAME} ${OFILES} ${LIBS}

all : ${EXTRA} ${NAME}
	@${ECHO} all done

extra : ${EXTRA}
	@${ECHO} done

depend.o : ${SRC}/depend.c ${SRCX}/config.h ${SRC}/depend.h\
 ${SRCX}/error.h ${SRC}/include.h ${SRC}/option.h ${SRC}/path.h\
 ${SRC}/types.h ${SRCX}/xalloc.h ${SRCX}/ossg.h ${SRCX}/ossg_api.h
	${MAIN_CC} ${SRC}/depend.c

error.o : ${SRCX}/error.c ${SRCX}/config.h ${SRCX}/error.h\
 ${SRCX}/release.h ${SRCX}/ossg.h ${SRCX}/ossg_api.h
	${MAIN_CC} ${SRCX}/error.c

include.o : ${SRC}/include.c ${SRCX}/config.h ${SRC}/depend.h\
 ${SRCX}/error.h ${SRC}/include.h ${SRC}/option.h ${SRC}/path.h\
 ${SRC}/tokens.h ${SRC}/types.h ${SRCX}/ossg.h ${SRCX}/ossg_api.h
	${MAIN_CC} ${SRC}/include.c

main.o : ${SRC}/main.c ${SRCX}/config.h ${SRC}/depend.h ${SRCX}/error.h\
 ${SRC}/option.h ${SRC}/output.h ${SRC}/path.h ${SRC}/types.h\
 ${SRCX}/xalloc.h ${SRCX}/ossg.h ${SRCX}/ossg_api.h
	${MAIN_CC} ${SRC}/main.c

option.o : ${SRC}/option.c ${SRCX}/config.h ${SRCX}/error.h\
 ${SRC}/include.h ${SRC}/option.h ${SRC}/path.h ${SRC}/types.h\
 ${SRCX}/xalloc.h ${SRCX}/ossg.h ${SRCX}/ossg_api.h
	${MAIN_CC} ${SRC}/option.c

