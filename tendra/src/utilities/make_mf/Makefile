 ${SRCX}/xalloc.h ${SRCX}/ossg.h ${SRCX}/ossg_api.h
	${MAIN_CC} ${SRCX}/xalloc.c

clean : ${EXTRA_CLEAN}
	${REMOVE} ${OFILES}
	${REMOVE} core

clobber : clean ${EXTRA_CLOBBER}
	${REMOVE} ${NAME}

${INIT} : ${SRC}/compile.ini
	@${REMOVE} ${INIT}
	${COPY} ${SRC}/compile.ini ${INIT}
